/* FEEDZY resets
-----------------------------*/
.rssCurrently,
.rssCurrently .feedzy-rss,
.rssCurrently .feedzy-rss ul,
.rssCurrently .feedzy-rss ul li,
.rssCurrently .feedzy-rss ul li .rss_image,
.rssCurrently .feedzy-rss ul li .rss_image a,
.rssCurrently .feedzy-rss ul li .rss_image a .default,
.rssCurrently .feedzy-rss ul li .rss_image a .fetched,
.rssCurrently .feedzy-rss ul li .title,
.rssCurrently .feedzy-rss ul li .title a,
.rssCurrently .feedzy-rss ul li .rss_content,
.rssCurrently .feedzy-rss ul li .rss_content p
{
  position: relative;
  float: none;
  padding: 0 !important;
  margin: 0;
}

/* FEEDZY RSS plugin
-----------------------------*/
/* display
----------*/

.rssCurrently .feedzy-rss ul li .rss_image a .default {
  display: none;
}

.sc_feed {
  width: 50%;
  float: left;
  min-height:700px;
  border: 1px solid RGB(200,200,200);
}

.sc_feed .feedzy-rss {
  margin-top: 12px;
}
